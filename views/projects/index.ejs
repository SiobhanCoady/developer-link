<%- include('../partials/header') %>

  <h1>Browse Projects</h1>

  <ul class="collapsible popout projects-list" data-collapsible="accordion">
    <% for (let project of projects) { %>
      <li>
        <div class="collapsible-header">
          <i class="material-icons">filter_drama</i>
          <%= project.title %>
        </div>
        <div class="collapsible-body">
          <span><%= project.description %></span>
          <p>Deadline: <%= project.deadline %></p>
          <p>GitHub: <%= project.github %></p>

          <p>Charity Type:
            <span><%= project.owner.Tags[0].name %></span>
          </p>

          <p>Technologies:
            <% for (let tag of project.Tags) { %>
              <% if (tag.tagType === 'technology') { %>
                <span><%= tag.name %></span>
              <% } %>
            <% } %>
          </p>

          <p>Languages:
            <% for (let tag of project.Tags) { %>
              <% if (tag.tagType === 'language') { %>
                <span><%= tag.name %></span>
              <% } %>
            <% } %>
          </p>

          <p>Non-profit:
            <a href="/users/<%= project.ownerId %>">
              <%= project.owner.orgName %>
            </a>
          </p>

        </div>
      </li>
    <% } %>



<%- include('../partials/footer') %>
